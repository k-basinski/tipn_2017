@article{K.2014,
abstract = {Rationale, aims and objectives: Measuring self-reported adherence may contribute to minimizing the risk of therapy failure. Hence, the main aim of the study was to assess the psychometric properties of the Czech version of Medication Adherence Report Scale (MARS-CZ) and its appropriateness for use in long-term statin therapy where goal levels of low-density lipoprotein cholesterol (LDL-c) should be achieved. Methods: Anonymous structured interview was performed to determine self-reported adherence by MARS-CZ in outpatients chronically treated with statins. At the same time, medication records were reviewed for inclusion of patients into groups of those who achieved and do not achieved LDL-c goal according to cardiovascular risk level. Reliability and validity of MARS-CZ were tested as well as the relationship between adherence and LDL-c goal achievement was examined. Results: A total of 136 (86.6{\%}) patients completed the interview; mean age was 66.1years; 49.3{\%} were male. The mean score of MARS-CZ was 24.4 and showed positive skewing. Satisfactory internal consistency (Cronbach's (alpha)=0.54), strong test-retest reliability (r=0.83, P{\textless}0.001; intra-class correlation=0.63, 95{\%} confidence interval: 0.35-0.81) and positive correlation with eight-item Morisky Medication Adherence Scale (r=0.62, P{\textless}0.001) were indicated. Low validity values were found between MARS-CZ and 12-item Short Form Health Survey mental and physical subscales. MARS-CZ score significantly correlated with LDL-c goal achievement (P{\textless}0.05) when all patients who achieved LDL-c goal (35{\%}) reported high adherence to statin. MARS-CZ score also correlated with cardiovascular risk level and doctor's judgments on adjusting treatment targets for each patient. Conclusion: This study proved MARS-CZ as an acceptable self-reported adherence measure. In routine clinical practice, MARS-CZ could be helpful to reveal medication non-adherence before the alteration of drug regimen and thereby contributing to enhancement of statin therapy management. (copyright) 2014 John Wiley {\&} Sons, Ltd.},
author = {K., Ladova and P., Matoulkova and Z., Zadak and K., Macek and P., Vyroubal and J., Vlcek and D.E., Morisky},
doi = {10.1111/jep.12201},
file = {:C$\backslash$:/Users/Krzysztof/MendeleyLibrary/2014/K. et al/K. et al. - 2014 - Self-reported adherence by MARS-CZ reflects LDL cholesterol goal achievement among statin users Validation study in t.pdf:pdf},
issn = {1356-1294},
journal = {Journal of Evaluation in Clinical Practice},
keywords = {Czech Republic,Short Form 12,achievement,astronomy,cardiovascular risk,clinical practice,confidence interval,decision making,drug therapy,human,hydroxymethylglutaryl coenzyme A reductase inhibit,implantable cardiac monitor,internal consistency,interview,low density lipoprotein,low density lipoprotein cholesterol,male,medication compliance,outpatient,patient,physician,reliability,risk,son,statin (protein),structured interview,test retest reliability,therapy,treatment failure,validation study,validity},
pages = {671--677},
title = {{Self-reported adherence by MARS-CZ reflects LDL cholesterol goal achievement among statin users: Validation study in the Czech Republic}},
url = {http://www.embase.com/search/results?subaction=viewrecord{\&}from=export{\&}id=L53191135{\%}5Cnhttp://dx.doi.org/10.1111/jep.12201{\%}5Cnhttp://sfx.hul.harvard.edu/sfx{\_}local?sid=EMBASE{\&}issn=13561294{\&}id=doi:10.1111/jep.12201{\&}atitle=Self-reported+adherence+by+MARS-CZ+r},
volume = {20},
