file = {:C$\backslash$:/Users/Krzysztof/MendeleyLibrary/2000/Vlaeyen, Linton/Vlaeyen, Linton - 2000 - Fear-avoidance and its consequences in chronic musculoskeletal pain A state of the art.pdf:pdf},
isbn = {0304-3959 (Print)$\backslash$n0304-3959 (Linking)},
