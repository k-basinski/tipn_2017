journal = {Pain},
keywords = {Adolescents,Arthritis,Children,Coping,Headache,Pain,Questionnaire},
number = {1-2},
pages = {83--96},
pmid = {9696461},
title = {{The pain coping questionnaire: Preliminary validation}},
volume = {76},
year = {1998}
}
