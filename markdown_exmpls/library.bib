@article{Leung2002,
abstract = { Examined whether or not pancultural dimensions based on general beliefs, or social axioms, can be identified in persons from cultures in 2 experiments. A Social Axioms Survey (SAS) was constructed, based on both previous psychological research primarily in Europe and North America on beliefs and qualitative research conducted in Hong Kong and Venezuela. In Exp 1, the SAS was administered to 358 Ss from Hong Kong and 203 Ss from Venezuela. In Exp 2, the SAS was administered to 114 American Ss, 99 German Ss, and 211 Ss from Japan. Factor analyses of these beliefs from student as well as adult samples revealed a pancultural, five-factor structure, with dimensions labeled as: cynicism, social complexity, reward for application, spirituality, and fate control. In Exp 2, this five-factor structure, with the possible exception of fate control, was replicated with college students from Japan, the US, and Germany. The potential implications of a universal, five-factor structure of individual social beliefs were discussed, along with the relation of this structure to indigenous belief systems and to culture-level analyses. (PsycINFO Database Record (c) 2010 APA, all rights reserved)},
author = {Leung, K. and Bond, M. H. and de Carrasquel, S. R. and Munoz, C. and Hernandez, M. and Murakami, F. and Yamaguchi, S. and Bierbrauer, G. and Singelis, T. M.},
