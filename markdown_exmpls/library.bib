month = {apr},
number = {7-8},
pages = {1044--52},
pmid = {23480500},
title = {{Women's experience of acute and chronic pain following breast cancer surgery.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23480500},
volume = {22},
year = {2013}
}
