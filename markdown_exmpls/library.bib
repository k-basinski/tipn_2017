isbn = {1526-5900},
issn = {15265900},
journal = {The Journal of Pain},
keywords = {Voxel-based morphometry,chronic back pain,gray matter volume},
number = {1},
pages = {111--118},
title = {{Chronic Back Pain Is Associated With Decreased Prefrontal and Anterior Insular Gray Matter: Results From a Population-Based Cohort Study}},
