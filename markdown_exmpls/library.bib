issn = {0887-0446},
journal = {Psychology {\&} Health},
keywords = {factor analysis,illness perceptions,ipq,ipq-r,musculoskeletal pain,questionnaire},
number = {January 2015},
pages = {1--19},
pmid = {22891727},
title = {{Musculoskeletal pain illness perceptions: Factor structure of the Illness Perceptions Questionnaire-Revised}},
volume = {28},
year = {2012}
