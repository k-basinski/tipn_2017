@article{Baliki2011,
abstract = {Chronic pain is maladaptive and influences brain function and behavior by altering the flow and integration of information across brain regions. Here we use a power spectral analysis to investigate impact of presence of chronic pain on brain oscillatory activity in humans. We examine changes in BOLD fluctuations, across different frequencies, in chronic back pain (CBP) patients (n ϭ 15) as compared to healthy controls (n ϭ 15) during resting-state fMRI. While healthy subjects exhibited a specific, frequency band-dependent, large-scale neural organization, patients showed increased high-frequency BOLD oscillations (0.12– 0.20 Hz) circumscribed mainly to medial pre-frontal cortex (mPFC) and parts of the default mode network. In the patients a correlation analysis related the mPFC aberrant BOLD high-frequency dynamics to altered functional connectivity to pain signaling/modulating brain regions, thus linking BOLD frequency changes to function. We also found that increased frequency fluctuations within the mPFC were temporally synchronous with sponta-neous pain changes in patients during a pain-rating task. These observations provide novel insights about the nature of CBP, identifying how it disturbs the resting brain, and link high-frequency BOLD oscillations to perception.},
author = {Baliki, Marwan N and Baria, Alex T and Apkarian, A Vania},
doi = {10.1523/JNEUROSCI.1984-11.2011},
file = {:C$\backslash$:/Users/Krzysztof/MendeleyLibrary/2011/Baliki, Baria, Apkarian/Baliki, Baria, Apkarian - 2011 - The Cortical Rhythms of Chronic Back Pain.pdf:pdf},
isbn = {1529-2401 (Electronic)$\backslash$r0270-6474 (Linking)},
issn = {0270-6474},
journal = {The Journal of Neuroscience},
number = {39},
pages = {13981--13990},
pmid = {21957259},
title = {{The Cortical Rhythms of Chronic Back Pain}},
volume = {31},
year = {2011}
}
