journal = {The journal of pain : official journal of the American Pain Society},
keywords = {SOREMO},
mendeley-tags = {SOREMO},
month = {oct},
number = {10},
pages = {1057--68},
pmid = {25077425},
title = {{Music modulation of pain perception and pain-related activity in the brain, brain stem, and spinal cord: a functional magnetic resonance imaging study.}},
