title = {{Delivering an Optimised Behavioural Intervention (OBI) to people with low back pain with high psychological risk; results and lessons learnt from a feasibility randomised controlled trial of Contextual Cognitive Behavioural Therapy (CCBT) vs. Physiotherap}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4468803{\&}tool=pmcentrez{\&}rendertype=abstract},
