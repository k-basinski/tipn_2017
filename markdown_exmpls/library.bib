@article{Seminowicz2011,
abstract = {Chronic pain is associated with reduced brain gray matter and impaired$\backslash$ncognitive ability. In this longitudinal study, we assessed whether$\backslash$nneuroanatomical and functional abnormalities were reversible and$\backslash$ndependent on treatment outcomes. We acquired MRI scans from chronic low$\backslash$nback pain (CLBP) patients before (n = 18) and 6 months after (spine$\backslash$nsurgery or facet joint injections; n = 14) treatment. In addition, we$\backslash$nscanned 16 healthy controls, 10 of which returned 6 months after the$\backslash$nfirst visit. We performed cortical thickness analysis on structural MRI$\backslash$nscans, and subjects performed a cognitive task during the functional$\backslash$nMRI. We compared patients and controls, as well as patients before$\backslash$nversus after treatment. After treatment, patients had increased cortical$\backslash$nthickness in the left dorsolateral prefrontal cortex (DLPFC), which was$\backslash$nthinner before treatment compared with controls. Increased DLPFC$\backslash$nthickness correlated with the reduction of both pain and physical$\backslash$ndisability. Additionally, increased thickness in primary motor cortex$\backslash$nwas associated specifically with reduced physical disability, and right$\backslash$nanterior insula was associated specifically with reduced pain. Left$\backslash$nDLPFC activity during an attention-demanding cognitive task was abnormal$\backslash$nbefore treatment, but normalized following treatment. These data$\backslash$nindicate that functional and structural brain abnormalities-specifically$\backslash$nin the left DLPFC-are reversible, suggesting that treating chronic pain$\backslash$ncan restore normal brain function in humans.},
author = {Seminowicz, David A and Wideman, Timothy H and Naso, Lina and Hatami-Khoroushahi, Zeinab and Fallatah, Summaya and Ware, Mark A and Jarzem, Peter and Bushnell, M Catherine and Shir, Yoram and Ouellet, Jean A and Stone, Laura S},
doi = {10.1523/JNEUROSCI.5280-10.2011},
file = {:C$\backslash$:/Users/Krzysztof/MendeleyLibrary/2011/Seminowicz et al/Seminowicz et al. - 2011 - Effective Treatment of Chronic Low Back Pain in Humans Reverses Abnormal Brain Anatomy and Function.pdf:pdf},
issn = {0270-6474},
journal = {Journal of Neuroscience},
number = {20},
pages = {7540--7550},
pmid = {21593339},
title = {{Effective Treatment of Chronic Low Back Pain in Humans Reverses Abnormal Brain Anatomy and Function}},
volume = {31},
year = {2011}
}
