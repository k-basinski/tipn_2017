@article{Raheem2015,
abstract = {Objectives To validate the effect of listening to music on perceived anxiety and pain during office based flexible cystoscopy utilizing the State-Trait Anxiety Inventory (STAI) and the Visual Analog Scale (VAS), in a well-matched North American veteran patient population in a prospective, randomized fashion. Subjects and methods A total of 137 veteran patients receiving routine urologic care in a North American Veterans Affairs (VA) healthcare system were recruited over a two-year period (June 2011 to June 2013). All patients were prospectively randomized to undergo office-based flexible cystoscopy with or without music. The music group consisted of 73 patients who listened to the same excerpt of classical music at time of flexible cystoscopy, and the non-music group consisted of 64 patients. Results The median post-procedural STAI anxiety scores between the music and non-music groups were statistically significant different: 30 (range 23-39) and 35 (range 28-49), respectively (p=0.0017). The median post-procedural pain VAS score between the music and non-music groups reached statistical significance: 0 (range 0-1) and 2 (range 1-2), respectively (p{\textless}0.0001). The median delta STAI anxiety score was statistically significantly different between the music and non-music groups: 0 (range -3-0) and 2 (range 0-4), respectively (p{\textless}0.0001). Conclusions This study demonstrates that listening to music decreases anxiety and pain associated with flexible cystoscopy in a North American VA patient population. We recommend incorporating music as an effective adjunct to other maneuvers utilized at time of flexible cystoscopy to reduce anxiety and pain.},
author = {Raheem, Omer and Mirheydar, Hossein S and Lee, Hak J and Patel, Nishant D and Godebu, Elana and Sakamoto, Kyoko},
doi = {10.1089/end.2015.0029},
issn = {1557-900X},
journal = {Journal of endourology / Endourological Society},
month = {jan},
pmid = {25630866},
title = {{Does listening to music during office-based flexible cystoscopy decrease anxiety in patients: A prospective randomized trial.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25630866},
year = {2015}
}
@article{Bruehl2009,
abstract = {Literature is reviewed indicating that greater tendency to manage anger via direct verbal or physical expression (trait anger-out) is associated with increased acute and chronic pain responsiveness. Neuroimaging data are overviewed supporting overlapping neural circuits underlying regulation of both pain and anger, consisting of brain regions including the rostral anterior cingulate cortex, orbitofrontal cortex, anterior insula, amygdala, and periaqueductal gray. These circuits provide a potential neural basis for observed positive associations between anger-out and pain responsiveness. The role of endogenous opioids in modulating activity in these interlinked brain regions is explored, and implications for understanding pain-related effects of anger-out are described. An opioid dysfunction hypothesis is presented in which inadequate endogenous opioid inhibitory activity in these brain regions contributes to links between trait anger-out and pain. A series of studies is presented that supports the opioid dysfunction hypothesis, further suggesting that gender and genetic factors may moderate these effects. Finally, possible implications of interactions between trait anger-out and state behavioral anger expression on endogenous opioid analgesic activity are described.},
author = {Bruehl, Stephen and Burns, John W and Chung, Ok Y and Chont, Melissa},
doi = {10.1016/j.neubiorev.2008.12.003},
file = {:C$\backslash$:/Users/Krzysztof/MendeleyLibrary/2009/Bruehl et al/Bruehl et al. - 2009 - Pain-related effects of trait anger expression neural substrates and the role of endogenous opioid mechanisms.pdf:pdf},
issn = {0149-7634},
journal = {Neuroscience and biobehavioral reviews},
keywords = {Analgesics, Opioid,Analgesics, Opioid: administration {\&} dosage,Anger,Anger: physiology,Brain,Brain Mapping,Brain: physiology,Expressed Emotion,Expressed Emotion: physiology,Humans,Magnetic Resonance Imaging,Neural Pathways,Neural Pathways: physiology,Opioid Peptides,Opioid Peptides: physiology,Pain,Pain Threshold,Pain Threshold: drug effects,Pain Threshold: physiology,Pain: drug therapy,Pain: genetics,Pain: physiopathology,Positron-Emission Tomography,Sex Characteristics},
month = {mar},
