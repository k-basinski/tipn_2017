journal = {Psychology and aging},
keywords = {Adult,Age Factors,Aged,Aged, 80 and over,Aging,Aging: psychology,Female,Humans,Male,Middle Aged,Psychomotor Performance,Time Factors,Time Perception,Young Adult},
month = {dec},
number = {4},
pages = {1152--63},
pmid = {23066805},
title = {{Late onset of age-related difference in unpaced tapping with no age-related difference in phase-shift error detection and correction.}},
